<div id="register" style = "width: 50%; margin: auto">
  <a href="#" id="home-link" class="waves-effect waves-light btn orange lighten-2"><i class="material-icons left">arrow back</i> Back to Home</a>
  <h1>Register</h1>
  <div class="error">
    {{#if error}}
      {{error}}
    {{/if}}
  </div>
  <form id="register-form">
    <div>Username: <input type="text" name="username" required /></div>
    <div>Password: <input type="password" name="password" required /></div>
    <div>Confirm Password: <input type="password" name="confirm" required /></div>
    <input type="submit" />
  </form>
</div>
