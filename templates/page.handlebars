<div id="freets" class = "container">

  <a href="#" id="home-link" class="waves-effect waves-light btn blue darken-3">
    <i class="fa fa-chevron-left"></i>
    Back to Home
  </a>

  <br/> <br/> <br/>

  <div class = "center-align" style ="font-size: 20px">Welcome to @{{user}}'s Fritter Page!</div>

<table class = "highlight">
  {{#each freets}}
    {{> freet_read}}
  {{else}}
    <p><em>This user has no freets!</em></p>
  {{/each}}
</table>

</div>
